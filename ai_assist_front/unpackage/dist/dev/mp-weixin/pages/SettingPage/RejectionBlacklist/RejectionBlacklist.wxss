	color: #FFFFFF;
	background:-webkit-linear-gradient(right,rgba(28,117,255,1),rgba(87,153,255,1));
	background:linear-gradient(-90deg,rgba(28,117,255,1),rgba(87,153,255,1));
	margin-left: 68rpx;
}
