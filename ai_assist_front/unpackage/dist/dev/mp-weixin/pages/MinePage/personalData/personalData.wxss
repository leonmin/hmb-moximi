
page {
	background-color: #F3F6F7;
}
.headItem {
	background-color: #FFFFFF;
	padding: 50rpx 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.headImg {
	width: 183rpx;
	height: 183rpx;
	border-radius: 200rpx;
	position: relative;
}
.imagBox{
	position: relative;
}
.cameraIcon{
	width: 57rpx;
	height: 57rpx;
	border-radius: 100rpx;
	position: absolute;
	right: 0;
	bottom: 10rpx;
}
.list {
	background-color: #FFFFFF;
	padding: 0 30rpx;
}
.listItem {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 47rpx 0;
	color: #111111;
	font-size: 28rpx;
}
.listLeft{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.listLeft>text{
	margin-left: 20rpx;
}
.listLeft>image {
	width: 55rpx;
	height: 55rpx;
	border-radius: 55rpx;
}
.listRight>text:nth-of-type(1) {
	margin-right: 40rpx;
}
.listRight>text:nth-of-type(2) {
	color: #999999;
	font-size: 34rpx;
}
.deliver {
	border-top: 1rpx solid #EDEDED;
}
.countBind {
	color: #111111;
	font-size: 30rpx;
	padding: 37rpx 30rpx;
	background-color: #F3F6F7;
}
.bindList {
	background-color: #FFFFFF;
	padding: 0 30rpx;
}
.bindItem{
	padding: 35rpx 0;
}
.sexBox{
	background-color: #FFFFFF;
	border-top-left-radius: 35rpx;
	border-top-right-radius: 35rpx;
}
.sexItem{
	padding: 30rpx;
	text-align: center;
}
.deliverPop{
	border-top: 2rpx solid #E9E9E9;
}
.eoTipText {
	color: #111111;
	font-size: 34rpx;
	font-weight: 600;
	display: inline-block;
	margin: 30rpx;
}
.eoTipBtn {
	margin: 40rpx 0 10rpx 0;
}
.eoTipBtn>button {
	padding: 38rpx 0;
}
.nickNameInput{
	text-align: left;
	border-bottom: 1rpx solid #e8e8e8;
	height: 60rpx;
	padding: 10rpx 30rpx;
}

